{
  "name": "portable-db-ts",
  "version": "1.0.2",
  "description": "A TypeScript library that provides a simple, in-memory database with a flexible API. It's designed to be portable, making it easy to integrate into your projects.",
  "author": "Dmitrii Baranov <dmitrii.a.baranov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/orimay/portable-db-ts.git"
  },
  "bugs": {
    "url": "https://github.com/orimay/portable-db-ts/issues"
  },
  "keywords": [
    "database",
    "typescript",
    "portable",
    "query",
    "index",
    "data-storage",
    "record-management",
    "collections",
    "sorting",
    "filtering",
    "typescript-library",
    "lightweight",
    "efficient",
    "data-modeling",
    "data-structure",
    "npm-module",
    "typescript-support",
    "database-library",
    "npm-package",
    "data-retrieval",
    "data-manipulation",
    "typescript-database",
    "npm-database",
    "nodejs",
    "record-store",
    "indexed-db"
  ],
  "type": "module",
  "main": "./dist/index",
  "module": "./dist/index",
  "types": "./dist/index",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pub": "npm run build && npm version patch --force && npm publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/faker": "^5.5.3",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "faker": "^5.5.3",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "async-utils-ts": "^1.0.1",
    "cache-ts": "^1.0.0",
    "mutex-ts": "^1.0.3",
    "radix-ts": "^1.0.2"
  }
}
